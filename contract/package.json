{
  "name": "dapp-token-economy-contract",
  "version": "0.1.0",
  "private": true,
  "description": "Contract for the token economy Dapp",
  "scripts": {
    "build": "exit 0",
    "test": "ava --verbose",
    "test:nyc": "nyc ava",
    "lint-fix": "yarn lint --fix",
    "lint-check": "yarn lint",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "lint:types": "tsc -p jsconfig.json",
    "lint-fix-jessie": "eslint -c '.eslintrc-jessie.js' --fix '**/*.js'",
    "lint-check-jessie": "eslint -c '.eslintrc-jessie.js' '**/*.js'"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "0.0.3",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "@agoric/install-ses": "*",
    "ava": "^3.12.1"
  },
  "dependencies": {
    "esm": "^3.2.5",
    "ses": "^0.10.1",
    "@agoric/assert": "*",
    "@agoric/captp": "*",
    "@agoric/ertp": "*",
    "@agoric/nat": "^2.0.1",
    "@agoric/store": "*",
    "@agoric/zoe": "*",
    "@agoric/bundle-source": "*",
    "@agoric/eventual-send": "*",
    "@agoric/deploy-script-support": "*"
  },
  "ava": {
    "files": [
      "test/**/test-*"
    ],
    "concurrency": 12,
    "failFast": false,
    "failWithoutAssertions": true,
    "verbose": true,
    "require": [
      "esm",
      "@agoric/install-ses"
    ],
    "nodeArguments": [
      "--trace-deprecation",
      "--napi-modules"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric/issues"
  },
  "homepage": "https://github.com/Agoric/agoric#readme",
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
