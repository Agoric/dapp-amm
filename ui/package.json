{
  "name": "agoric-amm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@agoric/assert": "*",
    "@agoric/captp": "*",
    "@agoric/ertp": "*",
    "@agoric/eventual-send": "*",
    "@agoric/install-ses": "*",
    "@agoric/nat": "^4.0.0",
    "@agoric/notifier": "*",
    "@agoric/same-structure": "*",
    "@agoric/ui-components": "*",
    "@agoric/zoe": "*",
    "@craco/craco": "^6.3.0",
    "@headlessui/react": "^1.4.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autodux": "^5.0.3",
    "eslint": "^7.32.0",
    "json5": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "ses": "^0.12.7",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "lint-check": "yarn lint",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "configure": "node use-on-chain-config",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "build:ses": "cp ../node_modules/ses/dist/lockdown.umd.js public/",
    "build:react": "yarn configure; yarn build:pre-patch; craco build; yarn build:post-patch",
    "build:pre-patch": "node -r esm ./ses-patch.js src/install-ses-lockdown.js",
    "build:post-patch": "node -r esm ./ses-patch.js build/index.html",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.12.1",
    "@endo/eslint-plugin": "^0.3.10",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14"
  },
  "eslintConfig": {
    "ignorePatterns": ["**/*.umd.js", "**/generated/*"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": "off"
    },
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx"]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": ["plugin:react/recommended"]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
