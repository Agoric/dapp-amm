{
  "name": "@agoric/dapp-autoswap-ui",
  "version": "0.0.1",
  "description": "Autoswap Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "scripts": {
    "clean": "rimraf ./build",
    "lint-check": "yarn lint",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "start": "react-scripts start",
    "test": "exit 0",
    "build": "exit 0",
    "build:ses": "cp ../node_modules/ses/dist/lockdown.umd.js public/",
    "build:react": "react-scripts build; node -r esm ./ses-patch.js build/index.html",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@agoric/ui-components": "*",
    "@agoric/install-ses": "*",
    "@agoric/assert": "*",
    "@agoric/captp": "*",
    "@agoric/ertp": "*",
    "@agoric/eventual-send": "*",
    "@agoric/notifier": "*",
    "@agoric/zoe": "*",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "autodux": "^5.0.3",
    "clsx": "^1.0.4",
    "ethers": "^5.0.25",
    "http-proxy-middleware": "^1.0.0",
    "json5": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-number-format": "^4.4.3",
    "react-router-dom": "^5.2.0",
    "ses": "^0.12.3"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.umd.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": "off"
    },
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "@agoric",
      "plugin:react/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
